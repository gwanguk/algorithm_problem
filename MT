#include <stdio.h>
#include <iostream>

using namespace std;


int func(int a, int b,int n, int k)
{
	while (1)
	{
		//a 공격 가능??
		if (a >= n)
			return 2;
		else if (a + b < n&&n <= a + b*k)
		{
			return 1;
		}
		else if (a+   b+ a <n&&  n<= a + b+a*k)
		{
			return 2;
		}
		else
		{
			n -= (a+b);
		}
	}
	//
}

int main(void) {

	//setbuf(stdout, NULL);
	int T;
	int test_case;
	int a;
	int b;
	int c;
	int N[7];
	int K[7];


	cin >> T;
	for (test_case = 1; test_case <= T; test_case++) {
		cin >> a; cin >> b; cin >> c;
		printf("Case #%d\n", test_case);
		for(int game =0;game<c;game++)
		{
			cin >> N[game];
			cin >> K[game];
		}
		for (int i = 0; i < c; i++)
		{
			int res = 0;
			res = func(a, b, N[i], K[i]);
			if (res == 1)
				cout << 'a';
			else
				cout << 'b';
		}
		cout << endl;

	}

	
	return 0;	// 정상종료 시 반드시 0을 리턴해야 합니다.
}
